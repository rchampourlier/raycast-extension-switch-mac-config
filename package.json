{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "switch-mac-config",
  "title": "Switch Mac Config",
  "description": "Quickly switch default browser and IDE via BetterTouchTool and Finicky",
  "icon": "command-icon.png",
  "author": "shudosagashi",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Switch Mac Config",
      "description": "Switch macOS config to the selected config",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.49.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
